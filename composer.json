{
    "name": "jbouzekri/config-knp-menu-bundle",
    "type": "symfony-bundle",
    "description": "Configure your knp menus with yaml files accross your project bundles",
    "keywords": ["Knp", "UI", "User Interface", "Menu", "Navigation", "Symfony"],
    "homepage": "https://github.com/jbouzekri/ConfigKnpMenuBundle",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "knplabs/knp-menu-bundle": "~3.0",
        "symfony/event-dispatcher": "^5.0|^6.0",
        "symfony/yaml": "^5.0|^6.0",
        "symfony/security-core": "^5.0|^6.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": { "Jb\\Bundle\\ConfigKnpMenuBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": {"Jb\\Bundle\\ConfigKnpMenuBundle\\Tests\\": "Tests/"}
    }
}
